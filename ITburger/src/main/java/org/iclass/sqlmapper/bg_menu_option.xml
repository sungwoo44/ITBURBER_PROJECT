<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="bg_option">
	  <select id="optionList" parameterType="String" resultType="org.iclass.vo.BG_MENU_OPTION">
 		SELECT *
		FROM BG_MENU_OPTION
		WHERE MENU_CODE = #{menu_code}
 	</select>
 </mapper>
 
 
 
 
 
 